{%- for var in vars %}
    {%- if loop.index0 == 0 -%}
        {%- set idx = "" -%}
    {%- else -%}
        {%- set idx = "+" ~ loop.index0 -%}
    {%- endif -%}
    {%- set fn = "inputs[" ~ vars_length ~ "*" ~ index_ident ~ idx ~ "]" %}
    {%- if var.var_type == "String" or var.var_type == "Word" %}
    let {{ var.ident }} = {{ fn }}.trim().to_string();
    {%- else %}
    let {{ var.ident }} = parse_input!({{ fn }}, {{ type_tokens[var.var_type] }});
    {%- endif -%}
{%- endfor %}
}
{%- endif %}
