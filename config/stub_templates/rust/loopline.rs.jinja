}
{%- else -%}

let mut inputs = String::new();
io::stdin().read_line(&mut inputs).unwrap();
for {{ index_ident }} in inputs.split_whitespace() {
    let {{ var.ident }} = parse_input!({{ index_ident }}, {{ type_tokens[var.var_type] }});
}

{%- endif -%}
