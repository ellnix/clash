{%- set len = vars | length -%}
{%- set uniques = vars | unique(attribute="var_type") -%}
{%- set one_type = uniques | length == 1 -%}
{%- set many_types = uniques | length > 1 -%}
{%- set unique_type = uniques | map(attribute="var_type") | first -%}
{{- vars | map(attribute="name") | join(sep=", ") }} = gets{% if len > 1 %}.split{% if one_type and unique_type != "Word" %}.map(&:{{ unique_type }}){% endif %}{% endif %}
{%- if many_types -%}
{%- for var in vars -%}
{%- if var.var_type != "String" and var.var_type != "Word" %}
{{var.name}} = {{var.name}}.{{var.var_type}}
{%- endif -%}
{%- endfor -%}
{%- endif -%}
